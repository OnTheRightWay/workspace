package com.lanou3g.homework;

public class Login {
	public UserInter login(String userName,String passWord){
		for(int i = 0; i<UserData.getUserData().length;i++){
			if(userName.equals(UserData.getUserData()[i][2])){
				if(passWord.equals(UserData.getUserData()[i][3])){
					SetJob.setJob(UserData.getUserData()[i][4]);
				}else System.out.println("密码错误");
			}else System.out.println("用户名不存在");
		}
	}
}
