package mod;
interface Food{
	public void eat();
}

class Apple implements Food{
	public void eat(){
		System.out.println("³ÔÆ»¹û");
	}
}
class Banana implements Food{
	public void eat(){
		System.out.println("³ÔÏã½¶");
	}
}

class Factory{
	public static Food getInstence(String s){
		if("Apple".equals(s)){
			return new Apple();
		}else if("Banana".equals(s)){
			return new Banana();
		}else return null;
	}
}

public class FactoryMod {
	public static void main(String[] args) {
		Food food=Factory.getInstence("ss");
		food.eat();
	}
}
