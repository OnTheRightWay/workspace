package family;

class Father{
	private String wife;
	private String son;
	private String name;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getWife() {
		return wife;
	}
	public void setWife(String wife) {
		this.wife = wife;
	}
	public String getSon() {
		return son;
	}
	public void setSon(String son) {
		this.son = son;
	}
}
class Mother{
	private String husband;
	private String son;
	private String name;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getHusband() {
		return husband;
	}
	public void setWife(String wife) {
		this.husband = husband;
	}
	public String getSon() {
		return son;
	}
	public void setSon(String son) {
		this.son = son;
	}
}


public class Family {

	public static void main(String[] args) {

	}

}
