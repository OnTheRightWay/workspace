package com.lanou3g.homework;

public class SetJob {
	public static UserInter setJob(){
		if("Woker".equals(UserData.getUserData()[i][4])){
			return new Worker();
		}else if("Doctor".equals(UserData.getUserData()[i][4])){
			return new Doctor();
		}else if("Cooker".equals(UserData.getUserData()[i][4])){
			return new Cooker();
		}else if("Boss".equals(UserData.getUserData()[i][4])){
			return new Boss();
		}
	}
	
}
