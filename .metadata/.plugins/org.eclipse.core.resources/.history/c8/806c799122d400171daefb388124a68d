package mod;
interface Fruit{
	public void eat();
}

class Apple implements Fruit{
	public void eat(){
		System.out.println("≥‘∆ªπ˚");
	}
}
class Banana implements Fruit{
	public void eat(){
		System.out.println("≥‘œ„Ω∂");
	}
}
class Cherry implements Fruit{
	public void eat(){
		System.out.println("≥‘”£Ã“");
	}
}
interface Factory{
	public static Fruit buyFruit();
}
class AppleFactory implements Factory{
	@Override
	public static Fruit buyFruit() {
		return new Apple();
	}
}
class BananaFactory implements Factory{
	@Override
	public static Fruit buyFruit() {
		return new Banana();
	}
}
class CherryFactory implements Factory{
	@Override
	public static Fruit buyFruit() {
		return new Cherry();
	}
}

public class FactoryDemo {
	public static void main(String[] args) {
		Food food=Factory.getInstence("ss");
		food.eat();
	}
}
