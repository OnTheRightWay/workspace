package LeiDemo;

public class Car {
	private String chePai;
	private String color;
	private int height;
	public void setChepai(String chePai){
		this.chePai=chePai;
	}
	public String getChePai(){
		return chePai;
	}
	public void setColor(String color){
		this.color=color;
	}
	public String getColor(){
		return color;
	}
	public void setHeight(int height){
		this.height=height;
	}
}
