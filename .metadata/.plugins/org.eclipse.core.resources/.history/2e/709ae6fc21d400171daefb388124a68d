package mod;
interface Fruit{
	public void eat();
}

class Apple implements Fruit{
	public void eat(){
		System.out.println("≥‘∆ªπ˚");
	}
}
class Banana implements Fruit{
	public void eat(){
		System.out.println("≥‘œ„Ω∂");
	}
}
class Cherry implements Fruit{
	public void eat(){
		System.out.println("≥‘”£Ã“");
	}
}
interface Factory{
	public Fruit buyFruit();
}
class AppleFactory implements Factory{
	public static Food getInstence(String s){
		if("Apple".equals(s)){
			return new Apple();
		}else if("Banana".equals(s)){
			return new Banana();
		}else return null;
	}
}

public class FactoryDemo {
	public static void main(String[] args) {
		Food food=Factory.getInstence("ss");
		food.eat();
	}
}
